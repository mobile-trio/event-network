{
  "expo": {
    "name": "Event-Network",
    "slug": "event-network-app",
    "description": "Mobile programing homework",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splashImg.png",
    "splash": {
      "image": "./assets/splashImg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.deonsky.eventnetworkapp"
    }
  }
}
